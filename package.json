{
    "name": "textguard",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "workspaces": [
        "server"
    ],
    "license": "MIT",
    "scripts": {
        "client:dev": "vite",
        "client:build": "vite build",
        "client:preview": "vite preview",
        "server:dev": "npm run dev --prefix server",
        "server:build": "npm run build --prefix server",
        "server:start": "npm run start --prefix server",
        "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
        "build": "npm run client:build && npm run server:build && npm run copy:client",
        "start": "npm run server:start"
    },
    "dependencies": {
        "bootstrap": "5.3.7",
        "dotenv": "17.2.0",
        "react": "19.1.0",
        "react-dom": "19.1.0",
        "react-icons": "5.5.0",
        "react-router-dom": "7.7.0"
    },
    "devDependencies": {
        "@eslint/js": "9.31.0",
        "@types/natural": "5.1.5",
        "@types/react": "19.1.8",
        "@types/react-dom": "19.1.6",
        "@types/uuid": "10.0.0",
        "@vitejs/plugin-react": "4.7.0",
        "concurrently": "9.2.0",
        "eslint": "9.31.0",
        "eslint-plugin-react-hooks": "5.2.0",
        "eslint-plugin-react-refresh": "0.4.20",
        "globals": "16.3.0",
        "sass": "1.89.2",
        "tar": "7.4.3",
        "typescript": "5.8.3",
        "typescript-eslint": "8.38.0",
        "vite": "7.0.5"
    },
    "overrides": {
        "rollup": "4.21.0"
    }
}
